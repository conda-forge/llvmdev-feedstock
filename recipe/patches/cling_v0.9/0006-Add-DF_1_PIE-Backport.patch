+++ b/interpreter/llvm/src/include/llvm/BinaryFormat/ELF.h
@@ -1289,7 +1289,8 @@ enum {
   DF_1_NORELOC = 0x00400000,
   DF_1_SYMINTPOSE = 0x00800000, // Object has individual interposers.
   DF_1_GLOBAUDIT = 0x01000000,  // Global auditing required.
