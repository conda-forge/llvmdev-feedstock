 interpreter/llvm/src/include/llvm/BinaryFormat/ELF.h | 3 ++-
 1 file changed, 2 insertions(+), 1 deletion(-)

diff --git a/interpreter/llvm/src/include/llvm/BinaryFormat/ELF.h b/interpreter/llvm/src/include/llvm/BinaryFormat/ELF.h
index 2bd711137845..39b352603465 100644
